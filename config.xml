<?xml version="1.0" encoding="UTF-8"?>
	<widget xmlns = "http://www.w3.org/ns/widgets"
		xmlns:gap = "http://phonegap.com/ns/1.0"
		id        = "com.rhswebdev.r3"
		versionCode="1.0"
		version   = "6.6">
		<name>R3</name>
		<description>
Roosevelt High School Hawaii Android App		</description>
		<author href="" email="webdevrhs@gmail.com">
RHS WebDev team
		</author>

		<preference name="fullscreen" value="false" />
		<preference name="webviewbounce" value="true" />
		<preference name="permissions" value="none" />
		<preference name="LoadUrlTimeoutValue" value="20000" />
		<preference name="disable-cursor" value="false" />
		<gap:platform name="android" />
		
		<gap:plugin name="onesignal-cordova-plugin" source="npm" />

<!-- Recommend the newest cli but requires cli-5.1.1+ and gradle for Android. -->
<!-- If the below settings are not compatible with your project set the "PGB ANT" version above. -->
<preference name="phonegap-version" value="cli-5.4.1" />
<preference name="android-build-tool" value="gradle" />
		
		
		
		
		
		<icon src="logo.png" gap:platform="android" gap:density="ldpi" />
		<icon src="logo.png" gap:platform="android" gap:density="mdpi" />
		<icon src="logo.png" gap:platform="android" gap:density="hdpi" />
		<icon src="logo.png" gap:platform="android" gap:density="xhdpi" />
		<icon src="logo.png" gap:platform="android" gap:density="xxhdpi" />
		<icon src="logo.png" gap:platform="android" gap:density="xxxhdpi" />
		<gap:splash src="splashscreen240.png" gap:platform="android" gap:density="ldpi" />
		<gap:splash src="splash.screen320.png" gap:platform="android" gap:density="mdpi" />
		<gap:splash src="splashscreen480.png" gap:platform="android" gap:density="hdpi" />
		<gap:splash src="splashscreen720.png" gap:platform="android" gap:density="xhdpi" />
		<gap:splash src="splashscreen1080.png" gap:platform="android" gap:density="xxhdpi" />
		<access origin="*" subdomains="true"  />
		<access origin="http://www.howardkam.com/lunch.js" />
		<access origin="http://howardkam.com" subdomains="true"  />
		<access origin="http://google.com" subdomains="true"  />
		<access origin="https://docs.google.com" subdomains="true"  />
		<access origin="google.com" />
		<gap:plugin name="com.phonegap.plugins.pushplugin" />

	</widget>
